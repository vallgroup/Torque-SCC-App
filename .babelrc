{
  "presets": ["@babel/preset-react", "@babel/preset-env"],
  "plugins": [
    "babel-plugin-styled-components",
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-optional-chaining",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "store": "./src/store",
          "components": "./src/components",
          "statics": "./src/statics",
          "theme": "./src/theme",
          "core": "./src/core"
        }
      }
    ]
  ]
}
